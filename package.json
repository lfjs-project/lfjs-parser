{
  "name": "@lfjs/parser",
  "version": "0.6.0",
  "description": "LFJS parser",
  "homepage": "https://lfjs.io",
  "author": "Paul Chavard",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "prettier": "prettier --single-quote --write src/*.js test/*.js",
    "clean": "rm -rf dist coverage",
    "lint": "eslint src test",
    "pretest": "yarn run lint",
    "test": "mocha --compilers js:babel-register",
    "build": "babel src --out-dir dist",
    "prepublish": "yarn run clean && yarn test && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfjs-project/lfjs-parser.git"
  },
  "keywords": [
    "lfjs",
    "es6",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/tchak/lfjs-parser/issues"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "mocha": "^3.2.0",
    "prettier": "^1.5.2"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
